pipeline {
    agent any     

    stages { 
        stage('docker compose up - dev') {     
             when { 
                branch 'develop'
            }       
            steps {
                 sh '''                  
                    docker-compose -f docker-compose.dev.yml up -d --build
                '''             
            }
        }        
    }

     post{
        always{
            sh 'docker image prune --all --filter until=48h -f'
        }
    }
}