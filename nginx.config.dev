

user    www;
worker_processes  1;
pid        logs/nginx.pid;
error_log  logs/error.log;


events {
    use           epoll;
    worker_connections  1024;
}




http {
    include       mime.types;
    charset       utf-8;

    access_log    logs/access.log ;

    server {
        listen   80;
        server_name   localhost;
       

        error_page    500 502 503 504  /50x.html;

        location      / {
            root      html;
        }

        location /identify {
          proxy_pass      http://127.0.0.1:8888;
        }

    }

}